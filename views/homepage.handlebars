<p>Home page</p>

<button class="“btn" btn-primary” id="“logout”" type="“submit”">signup</button>

<form class="form user-form">
  <div class="form-group">
    <label for="userPost">User post:</label>
    <input class="form-input" type="text" id="userPost" />
  </div>

  <div class="form-group">
    <button class="btn btn-primary" type="submit">Post</button>
  </div>
</form>
{{#each posts as |post|}}
  <div class="post" data-postid="{{post.id}}">
    <div class="post-content">
      <p>
        {{post.user.name}}:
        {{post.content}}
      </p>
    </div>
  </div>
{{/each}}

<script src="./js/homePage.js"></script>